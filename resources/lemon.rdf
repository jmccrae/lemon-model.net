        <rdfs:label xml:lang="de">Konstituent</rdfs:label>
        <rdfs:label xml:lang="en">Constituent</rdfs:label>
        <rdfs:label xml:lang="es">Constituyente</rdfs:label>
        <rdfs:label xml:lang="fr">Constituent</rdfs:label>
        <rdfs:label xml:lang="nl">Constituent</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LemonElement"/>
        <rdfs:comment xml:lang="en">The class of constituents, that is types applied to nodes in a phrase structure graph</rdfs:comment>
    </rdfs:Class>
    


    <!-- http://lemon-model.net/lemon#Part -->

    <rdfs:Class rdf:about="http://lemon-model.net/lemon#Part">
        <rdf:type rdf:resource="&owl;Class"/>
