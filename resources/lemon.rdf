        <rdfs:label xml:lang="es">Parte de la palabra</rdfs:label>
        <rdfs:label xml:lang="fr">Partie du mot</rdfs:label>
        <rdfs:label xml:lang="nl">Deel van een woord</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LexicalEntry"/>
        <rdfs:comment xml:lang="en">An affix is a morpheme that is attached to a word stem to form a new word.  Use this for lexical entries with only abstract forms</rdfs:comment>
    </rdfs:Class>
    


    <!-- http://lemon-model.net/lemon#Phrase -->

    <rdfs:Class rdf:about="http://lemon-model.net/lemon#Phrase">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdfs:label xml:lang="de">Phrase</rdfs:label>
        <rdfs:label xml:lang="en">Phrase</rdfs:label>
        <rdfs:label xml:lang="es">Sintagma</rdfs:label>
        <rdfs:label xml:lang="fr">Syntagme</rdfs:label>
        <rdfs:label xml:lang="nl">Zinsdeel</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LexicalEntry"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://lemon-model.net/lemon#decomposition"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A phrase in lemon is defined in the looser sense of a sequence of words, it does not have to a fully grammatical phrase</rdfs:comment>
    </rdfs:Class>
    


    <!-- http://lemon-model.net/lemon#PhraseElement -->

    <rdfs:Class rdf:about="http://lemon-model.net/lemon#PhraseElement">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdfs:label xml:lang="de">Phrase-Element</rdfs:label>
        <rdfs:label xml:lang="en">Phrase element</rdfs:label>
        <rdfs:label xml:lang="es">Elemento del sintagma</rdfs:label>
        <rdfs:label xml:lang="fr">Elément du syntagme</rdfs:label>
        <rdfs:label xml:lang="nl">Zinselement</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LemonElement"/>
        <rdfs:comment xml:lang="en">A terminal node in a phrase structure graph, i.e., a realisable, lexical element.</rdfs:comment>
    </rdfs:Class>
    


    <!-- http://lemon-model.net/lemon#PropertyValue -->

    <rdfs:Class rdf:about="http://lemon-model.net/lemon#PropertyValue">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdfs:label xml:lang="de">Prädikatswert</rdfs:label>
        <rdfs:label xml:lang="en">Property Value</rdfs:label>
        <rdfs:label xml:lang="es">Valor de la propiedad</rdfs:label>
        <rdfs:label xml:lang="fr">Valeur de la propriété</rdfs:label>
        <rdfs:label xml:lang="nl">Waarde van het predikaat</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LemonElement"/>
        <owl:disjointWith rdf:resource="http://lemon-model.net/lemon#SenseDefinition"/>
        <owl:disjointWith rdf:resource="http://lemon-model.net/lemon#UsageExample"/>
        <rdfs:comment xml:lang="en">A value that can be used in the range of linguistic property</rdfs:comment>
    </rdfs:Class>
    


    <!-- http://lemon-model.net/lemon#Prototype -->

    <rdfs:Class rdf:about="http://lemon-model.net/lemon#Prototype">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdfs:label xml:lang="en">Prototype</rdfs:label>
        <rdfs:label xml:lang="es">Prototipo</rdfs:label>
        <rdfs:label xml:lang="fr">Prototype</rdfs:label>
        <rdfs:label xml:lang="nl">Prototyp</rdfs:label>
        <rdfs:label xml:lang="nl">Prototype</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LemonElement"/>
    </rdfs:Class>
    


    <!-- http://lemon-model.net/lemon#SenseCondition -->

    <rdfs:Class rdf:about="http://lemon-model.net/lemon#SenseCondition">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdfs:label xml:lang="de">Bedingung</rdfs:label>
        <rdfs:label xml:lang="en">Condition</rdfs:label>
        <rdfs:label xml:lang="es">Condición</rdfs:label>
        <rdfs:label xml:lang="fr">Condition</rdfs:label>
        <rdfs:label xml:lang="nl">Voorwaarde</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LemonElement"/>
        <rdfs:comment xml:lang="en">Indicates a logical condition that is used indicate when a particular term has the given meaning</rdfs:comment>
    </rdfs:Class>
    


    <!-- http://lemon-model.net/lemon#SenseContext -->

    <rdfs:Class rdf:about="http://lemon-model.net/lemon#SenseContext">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdfs:label xml:lang="de">Kontext</rdfs:label>
        <rdfs:label xml:lang="en">Context</rdfs:label>
        <rdfs:label xml:lang="es">Contexto</rdfs:label>
        <rdfs:label xml:lang="fr">Contexte</rdfs:label>
        <rdfs:label xml:lang="nl">Context</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LemonElement"/>
        <rdfs:comment xml:lang="en">Indicates the context in which a term is to be used. The context refers not to the immediate syntactic context, but the document and register the document is used in</rdfs:comment>
    </rdfs:Class>
    


    <!-- http://lemon-model.net/lemon#SenseDefinition -->

    <rdfs:Class rdf:about="http://lemon-model.net/lemon#SenseDefinition">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdfs:label xml:lang="de">Definition</rdfs:label>
        <rdfs:label xml:lang="en">Definition</rdfs:label>
        <rdfs:label xml:lang="es">Definición</rdfs:label>
        <rdfs:label xml:lang="fr">Définition</rdfs:label>
        <rdfs:label xml:lang="nl">Definitie</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LemonElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://lemon-model.net/lemon#value"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://lemon-model.net/lemon#SynRoleMarker"/>
        <owl:disjointWith rdf:resource="http://lemon-model.net/lemon#UsageExample"/>
        <rdfs:comment xml:lang="en">A definition of a sememe, that is the a text describing the exact meaning of the lexical entry when its sense is the given ontology reference</rdfs:comment>
    </rdfs:Class>
    


    <!-- http://lemon-model.net/lemon#SynRoleMarker -->

    <rdfs:Class rdf:about="http://lemon-model.net/lemon#SynRoleMarker">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdfs:label xml:lang="de">Syntactische Rolle-Marker</rdfs:label>
        <rdfs:label xml:lang="en">Syntactic role marker</rdfs:label>
        <rdfs:label xml:lang="es">Marcador de la función sintáctica</rdfs:label>
        <rdfs:label xml:lang="fr">Marqueur du rôle syntaxique</rdfs:label>
        <rdfs:label xml:lang="nl">Syntactisch rol merker</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LemonElement"/>
        <rdfs:subClassOf>
            <rdfs:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://lemon-model.net/lemon#LexicalEntry"/>
                    <rdf:Description rdf:about="http://lemon-model.net/lemon#PropertyValue"/>
                </owl:unionOf>
            </rdfs:Class>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://lemon-model.net/lemon#UsageExample"/>
        <rdfs:comment xml:lang="en">The indicator of a given syntactic argument, normally a preposition or other particle marker or a linguistic property such as case</rdfs:comment>
    </rdfs:Class>
    


    <!-- http://lemon-model.net/lemon#UsageExample -->

    <rdfs:Class rdf:about="http://lemon-model.net/lemon#UsageExample">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdfs:label xml:lang="de">Anwendungsbeispiel</rdfs:label>
        <rdfs:label xml:lang="en">Usage Example</rdfs:label>
        <rdfs:label xml:lang="es">Ejemplo de uso</rdfs:label>
        <rdfs:label xml:lang="fr">Exemple d&#39;utilisation</rdfs:label>
        <rdfs:label xml:lang="nl">Voorbeeld van het gebruik</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LemonElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://lemon-model.net/lemon#value"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An example of the usage of a lexical entry when refering to the ontology entity given by the sememe&#39;s reference. This should in effect be an example of the form used in context. E.g., &quot;this is a *usage example*&quot;</rdfs:comment>
    </rdfs:Class>

    <!-- http://lemon-model.net/lemon#Word -->

    <rdfs:Class rdf:about="http://lemon-model.net/lemon#Word">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdfs:label xml:lang="de">Wort</rdfs:label>
        <rdfs:label xml:lang="en">Word</rdfs:label>
        <rdfs:label xml:lang="es">Palabra</rdfs:label>
        <rdfs:label xml:lang="fr">Mot</rdfs:label>
        <rdfs:label xml:lang="nl">Woord</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://lemon-model.net/lemon#LexicalEntry"/>
        <rdfs:comment xml:lang="en">A word is a single unit of writing or speech. In languages written in Latin, Cyrillic, Greek, Arabic scripts etc. these are assumed to be separated by white-space characters. For Chinese, Japanese, Korean this should correspond to some agreed segmentation scheme.</rdfs:comment>
    </rdfs:Class>
    

</rdf:RDF>
